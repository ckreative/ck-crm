APP_NAME="Laravel Admin"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=pgsql

# For local Supabase (recommended for development)
# Run 'supabase start' to get these credentials
PGHOST=127.0.0.1
PGPORT=54322
PGDATABASE=postgres
PGUSER=postgres
PGPASSWORD=postgres
PGSCHEMA=laravel
PGSSLMODE=prefer
PGCHANNELBINDING=prefer

# For Supabase Cloud (production)
# Get these from your Supabase project settings
# PGHOST=db.xxxxxxxxxxxxxxxxxxxx.supabase.co
# PGHOST_DIRECT=db.xxxxxxxxxxxxxxxxxxxx.supabase.co
# PGPORT=5432
# PGDATABASE=postgres
# PGUSER=postgres
# PGPASSWORD=your-supabase-password
# PGSCHEMA=laravel
# PGSSLMODE=require
# PGCHANNELBINDING=require

# Session Configuration
SESSION_DRIVER=database
SESSION_LIFETIME=43200
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=public
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

# Redis Configuration (for Docker setup)
REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Email Configuration (Resend)
MAIL_MAILER=resend
RESEND_API_KEY=your-resend-api-key
MAIL_FROM_ADDRESS="noreply@yourdomain.com"
MAIL_FROM_NAME="${APP_NAME}"

# Storage Configuration
# For local development, use 'public'
# For production with R2, use 'r2' and Laravel Cloud will inject AWS_* variables
# FILESYSTEM_DISK=r2

# AWS/R2 Configuration (Laravel Cloud provides these automatically)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=auto
AWS_BUCKET=
AWS_URL=
AWS_ENDPOINT=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Frontend Configuration
VITE_APP_NAME="${APP_NAME}"

# Cal.com Integration
CALCOM_API_KEY=